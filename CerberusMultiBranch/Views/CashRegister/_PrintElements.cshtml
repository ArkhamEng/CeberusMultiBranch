@model CerberusMultiBranch.Models.ViewModels.Operative.PrintableViewModel
@using CerberusMultiBranch.Support

@if (Model.HasCreditNote)
{
    @Html.Partial("_PrintCreditNote", Model.SaleCreditNote)
    <script>
        PrintCreditNote();
    </script>
}

@if (Model.PrintType == Cons.One)
{
    @Html.Partial("_PrintTicket", Model.Sale)
    <script>
        PrintNote();
    </script>
}

else
{
   @Html.Partial("_PrintNote", Model.Sale)
    <script>
        PrintNote();
    </script>
}
